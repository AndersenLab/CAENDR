{
    "paths": {
        "site_prefix":              "//storage.googleapis.com",
        "release_path":             "caendr-site-public-bucket/dataset_release/c_$SPECIES/$RELEASE",
        "bam_bai_path":             "caendr-site-private-bucket/bam",
        "fasta_filename":           "browser_tracks/c_$SPECIES.$PRJ.$WB.genome.fa"
    },
    "tracks": {
        "Genes": {
            "filename":             "browser_tracks/$SPECIES_gene.$WB.bed.gz",
            "checked":              true,
            "params": {
                "name":             "Genes",
                "indexed":          false,
                "order":            1,
                "height":           65,
                "color":            "#5C5CD6",
                "displayMode":      "EXPANDED",
                "searchable":       false
            }
        },
        "Transcripts": {
            "filename":             "browser_tracks/$SPECIES_transcripts_$WB.bed.gz",
            "checked":              true,
            "params": {
                "name":             "Transcripts",
                "indexed":          true,
                "order":            2,
                "color":            "#A366FF",
                "displayMode":      "SQUISHED",
                "searchable":       false
            }
        },
        "Divergent Regions Summary": {
            "filename":             "divergent_regions_all.$RELEASE.bed",
            "checked":              false,
            "params": {
                "name":             "Divergent Regions Summary",
                "order":            6,
                "height":           40,
                "color":            "#CB3466",
                "displayMode":      "EXPANDED",
                "visibilityWindow": 20000000
            }
        },
        "Divergent Regions": {
            "filename":             "divergent_regions_strain.$RELEASE.bed",
            "checked":              false,
            "source": {
                "title":            "Lee et al. 2020",
                "url":              "https://andersenlab.org/publications/2021LeeNatureEE.pdf"
            },
            "params": {
                "name":             "Divergent Regions",
                "order":            6,
                "height":           200,
                "color":            "#9B4763",
                "displayMode":      "EXPANDED",
                "visibilityWindow": 20000000
            }
        }
    },
    "templates": {
        "$STRAIN": {
            "filename":             "strain/vcf/$STRAIN.$RELEASE.vcf.gz",
            "index_suffix":         ".tbi",
            "checked":              false,
            "params": {
                "name":             "$STRAIN",
                "order":            100,
                "displayMode":      "EXPANDED",
                "color":            "#ffffff",
                "homvarColor":      "#0066ff",
                "homrefColor":      "#c2c2d6",
                "visibilityWindow": 20000,
                "searchable":       false
            }
        },
        "${STRAIN}_bam": {
            "filename":             "c_$SPECIES/$STRAIN.bam",
            "is_bam":               true,
            "index_suffix":         ".bai",
            "checked":              false,
            "params": {
                "id":               "${STRAIN}_bam",
                "name":             "${STRAIN}_bam",
                "order":            100,
                "visibilityWindow": 20000,
                "searchable":       false
            }
        }
    }
}